        <th>
            @Html.DisplayNameFor(model => model.A9)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.A10)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.estado)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
