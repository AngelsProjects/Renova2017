        <td>
            @Html.DisplayFor(modelItem => item.A8)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.A9)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.A10)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.estado)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.IDAsistencia }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.IDAsistencia })
        </td>
    </tr>
}

</table>
