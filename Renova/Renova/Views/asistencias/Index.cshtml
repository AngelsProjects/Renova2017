@model IEnumerable<RenoEnergy.Models.asistencia>

@{
    ViewBag.Title = "Index";
}

<h2>Asistencias</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.alumno.nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.A1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.A2)
